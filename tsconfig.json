{
    "compileOnSave": true,
    "compilerOptions": {
        "declaration": false,
        "inlineSourceMap": false,
        "inlineSources": false,
        "lib":[
            "es2015",
            "es2016"
        ],
        "module": "commonjs",
        "noImplicitAny": true,
        "outDir": "dist",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "strictNullChecks": true,
        "target": "es2016"
    },
    "include": [
        "src/**/*.ts"
    ]
}